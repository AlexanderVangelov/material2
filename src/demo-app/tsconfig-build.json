{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "lib": [
      "es6",
      "es2015",
      "dom"
    ],
    "skipLibCheck": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "outDir": "../../dist/packages/demo-app",
    "sourceMap": true,
    "target": "es5",
    "stripInternal": false,
    "typeRoots": [
      "../../node_modules/@types/!(node)"
    ],
    "baseUrl": ".",
    "paths": {
      "@angular/material/*": [
        "../../dist/packages/material/*"
      ],
      "@angular/material": [
        "../../dist/packages/material"
      ],
      "@angular/cdk/*": [
        "../../dist/packages/cdk/*"
      ],
      "@angular/cdk": [
        "../../dist/packages/cdk"
      ],
      "@angular/material-experimental/*": [
        "../../dist/packages/material-experimental/*"
      ],
      "@angular/material-experimental": [
        "../../dist/packages/material-experimental"
      ],
      "@angular/cdk-experimental/*": [
        "../../dist/packages/cdk-experimental/*"
      ],
      "@angular/cdk-experimental": [
        "../../dist/packages/cdk-experimental"
      ],
      "@angular/material-moment-adapter": [
        "../../dist/packages/material-moment-adapter"
      ],
      "@angular/material-examples": [
        "../../dist/packages/material-examples"
      ]
    }
  },
  "files": [
    "./demo-app-types.d.ts",
    "./demo-app-module.ts",
    "./system-config.ts",
    "./main.ts"
  ],
  "angularCompilerOptions": {
    "enableIvy": "ngtsc"
  }
}